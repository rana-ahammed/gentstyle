{
    "env": {
        "node": true,
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": ["airbnb-base", "plugin:prettier/recommended"],

    "plugins": ["prettier"],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "import/no-extraneous-dependencies": [
        "error",
        { "devDependencies": false, "optionalDependencies": false, "peerDependencies": false }
    ],
    "rules": {
        "no-console": "off",
        "import/extensions": "off"
    }
}
